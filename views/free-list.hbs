<h2>Free Stuff</h2>

<form action="/free-list" method="get">
  <select name="category">
    <option value="Event">Event</option>
    <option value="Culture">Culture</option>
    <option value="Sports">Sports</option>
    <option value="Food">Food</option>
  </select>
  <button>Filter</button>
</form>
<button>
  <a href="/free-list">See all</a>
</button>

{{#each resources}}
<div>
  <img src="/images/free-icon.png" style="height:50px" alt="default img">
  <h3> {{this.category}} </h3>
  <h5> {{this.shortdescr}} </h5>
  <p> {{this.location}} </p>
  <p> {{this.date}} </p>
  <a href="{{this._id}}/detail"> Details</a>
</div>
{{/each}}